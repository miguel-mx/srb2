{% extends 'base.html.twig' %}

{% block body %}
    <h1>Fi</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fi.id }}</td>
            </tr>
            <tr>
                <th>Origen</th>
                <td>{{ fi.origen }}</td>
            </tr>
            <tr>
                <th>Year</th>
                <td>{{ fi.year }}</td>
            </tr>
            <tr>
                <th>Fi</th>
                <td>{{ fi.fi }}</td>
            </tr>
            <tr>
                <th>Cuartil</th>
                <td>{{ fi.cuartil }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('fi_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('fi_edit', { 'id': fi.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
