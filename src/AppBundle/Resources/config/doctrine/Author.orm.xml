<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Author" table="Author">
    <unique-constraints>
      <unique-constraint name="UNIQ_BA03DDFEA76ED395" columns="user_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="120" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="alias" type="text" column="alias" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="user" target-entity="FosUser" fetch="LAZY">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="referencia" target-entity="Referencia" mapped-by="author" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
